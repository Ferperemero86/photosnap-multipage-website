{
	"extends": [
		"standard",
		"plugin:react/recommended",
		"plugin:prettier/recommended"
	],
	"env": {
		"browser": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		],
		"react/prop-types": "off",
		"no-Tabs": true,
		"react/jsx-first-prop-new-line": "always"
	}
}